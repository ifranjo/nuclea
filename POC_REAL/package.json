{
  "name": "nuclea-poc-real",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3002",
    "build": "next build",
    "start": "next start -p 3002",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "db:types": "npx supabase gen types typescript --local > src/lib/database.types.ts",
    "seed": "npx tsx scripts/seed.ts",
    "test:ceo": "node tests/ceo_bug_verification_final.mjs",
    "test:beta": "node tests/beta_qa.mjs",
    "test:upload": "node tests/upload_flow_e2e.mjs",
    "test:send-claim": "node tests/smoke_send_claim.mjs",
    "test:playwright": "playwright test",
    "test:e2e": "npx playwright test tests/e2e/",
    "test:e2e:headed": "npx playwright test tests/e2e/ --headed",
    "test:e2e:ui": "npx playwright test tests/e2e/ --ui",
    "test:install": "playwright install chromium",
    "test:health": "node tests/healthcheck.mjs",
    "soak:dry-run": "powershell -ExecutionPolicy Bypass -File scripts/soak_10h_runner.ps1 -DurationHours 0.1 -SleepSeconds 30",
    "soak:10h": "powershell -ExecutionPolicy Bypass -File scripts/soak_10h_runner.ps1 -DurationHours 10 -SleepSeconds 120"
  },
  "dependencies": {
    "@supabase/ssr": "^0.6.0",
    "@supabase/supabase-js": "^2.49.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^11.15.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.469.0",
    "next": "^15.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@playwright/test": "^1.58.2",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.1.0",
    "playwright": "^1.58.2",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  }
}
